    "description": "Motivational quotes for the day.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NehalPatel\\Motivation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NehalPatel\\Motivation\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nehal Patel",
            "email": "patelnehal4u@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.11"
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "NehalPatel\\Motivation\\Providers\\MotivationAppServiceProvider"
            ],
            "aliases" : {
                "Motivation": "NehalPatel\\Motivation\\Facades\\Motivation"
            }
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
